{
  "name": "@meeting-management/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "dev": "nx run-many -t dev,serve --projects=frontend,backend --parallel=2",
    "dev:frontend": "nx dev frontend",
    "dev:backend": "nx serve backend",
    "build": "nx run-many -t build --all",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "build:prod": "nx run-many -t build --all --skip-nx-cache",
    "build:frontend:prod": "nx build frontend --configuration=production --skip-nx-cache",
    "build:backend:prod": "nx build backend --prod --skip-nx-cache",
    "lint": "nx run-many -t lint --all",
    "lint:fix": "nx run-many -t lint --all --fix",
    "test": "nx run-many -t test --all",
    "test:watch": "nx run-many -t test --all --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "typecheck": "nx run-many -t typecheck --all"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nx/eslint-plugin": "^22.0.1",
    "@nx/express": "^22.0.1",
    "@nx/jest": "^22.0.1",
    "@nx/js": "22.0.1",
    "@nx/next": "^22.0.1",
    "@nx/node": "^22.0.1",
    "@nx/react": "^22.0.1",
    "@nx/web": "22.0.1",
    "@nx/webpack": "22.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "~15.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nx": "22.0.1",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.6.0",
    "express": "^4.21.2",
    "next": "~15.2.4",
    "prisma": "^6.18.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
